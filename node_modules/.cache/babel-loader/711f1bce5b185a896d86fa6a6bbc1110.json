ot name=\"header\">&ndash;&gt;-->\n                            <!--&lt;!&ndash;default header&ndash;&gt;-->\n                        <!--&lt;!&ndash;</slot>&ndash;&gt;-->\n                    <!--&lt;!&ndash;</div>&ndash;&gt;-->\n\n                    <!--<div class=\"modal-body\">-->\n                        <!--<slot name=\"body\">-->\n                            <!--default body-->\n                        <!--</slot>-->\n                    <!--</div>-->\n\n                    <!--<div class=\"modal-footer\">-->\n                        <!--<slot name=\"footer\">-->\n                            <!--<button class=\"modal-default-button\" @click=\"$emit('close')\">-->\n                                <!--OK-->\n                            <!--</button>-->\n                        <!--</slot>-->\n                    <!--</div>-->\n\n                    <div class=\"server-error-msg__wrap\">\n                        <div class=\"server-error-msg\">\n                            <h3>\n                                Помилка на сервері -\n                            </h3>\n                            <h4>\n                                Спробуйте пізніше\n                            </h4>\n                        </div>\n                    </div>\n                    <div class=\"aliens-img\">\n                        <img src=\"../../../assets/ServerErrorGray.png\" alt=\"\">\n                    </div>\n                    <div class=\"close-modal\" @click=\"$emit('close')\">\n                        <button>Ok</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </transition>\n</template>\n\n<script>\nexport default {\n  name: 'index'\n}\n</script>\n\n<style scoped lang=\"scss\">\n    .modal-mask {\n        position: fixed;\n        z-index: 10000;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n        background-color: rgba(0, 0, 0, .5);\n        display: table;\n        transition: